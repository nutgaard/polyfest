{
  "name": "polyfest",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://utgaard.xyz/polyfest",
  "scripts": {
    "start:backend": "nodemon --watch server server/server.js",
    "start:backendbuild": "gulp watch",
    "start:frontend": "react-scripts-ts start",
    "build:backend": "gulp",
    "build:frontend": "react-scripts-ts build",
    "start": "npm-run-all -p start:*",
    "dev": "cross-env REACT_APP_MOCK=true REACT_APP_ENV=dev npm-run-all -p start:frontend",
    "build": "npm-run-all -p build:*",
    "deploy": "cross-env REACT_APP_MOCK=true REACT_APP_ENV=dev react-scripts-ts build && gh-pages -d build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "2d-polygon-boolean": "^1.0.1",
    "body-parser": "^1.18.2",
    "color": "^2.0.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "fetch-mock": "^5.12.2",
    "greiner-hormann": "^1.3.4",
    "leaflet": "^1.3.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.11",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-leaflet": "^1.8.0",
    "react-scripts-ts": "2.12.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/color": "^2.0.0",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.0",
    "@types/fetch-mock": "^5.12.2",
    "@types/jest": "^22.0.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.3.0",
    "@types/query-string": "^5.0.1",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-leaflet": "^1.1.4",
    "cross-env": "^5.1.3",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^4.0.0-alpha.1",
    "npm-run-all": "^4.1.2"
  }
}
